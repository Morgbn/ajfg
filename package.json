{
  "name": "ajfg",
  "description": "Form generator based on JSON Schema",
  "license": "MIT",
  "keywords": [
    "vue",
    "vuejs3",
    "json-schema",
    "ui",
    "ui-components",
    "ui-library",
    "form"
  ],
  "version": "0.0.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/ajfg-ts.umd.js",
  "module": "./dist/ajfg-ts.es.js",
  "exports": {
    ".": {
      "import": "./dist/ajfg.es.js",
      "require": "./dist/ajfg.umd.js"
    },
    "./dist/ajfg.css": {
      "import": "./dist/ajfg.css",
      "require": "./dist/ajfg.css"
    }
  },
  "types": "./dist/main.d.ts",
  "scripts": {
    "dev": "vitepress dev docs",
    "lint": "eslint --ext .js,.ts,.vue .",
    "build": "vue-tsc && vite build",
    "prepack": "vite optimize"
  },
  "peerDependencies": {
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@types/node": "^18.13.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "ajv-i18n": "^4.2.0",
    "eslint": "^8.33.0",
    "eslint-plugin-jsdoc": "^39.8.0",
    "json-schema": "^0.4.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "typescript": "^4.9.3",
    "unplugin-auto-import": "^0.13.0",
    "unplugin-vue-components": "^0.23.0",
    "vite": "^4.1.0",
    "vite-plugin-dts": "^1.7.2",
    "vitepress": "^1.0.0-alpha.45",
    "vue": "^3.2.47",
    "vue-tsc": "^1.0.24"
  },
  "dependencies": {
    "ajv": "^8.12.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/morgbn/ajfg.git"
  }
}
