{
  "name": "j2u",
  "description": "Json schema Ui Utilities (Form generator, List, Filter)",
  "license": "MIT",
  "keywords": [
    "vue",
    "vuejs3",
    "json-schema",
    "ui",
    "ui-components",
    "ui-library",
    "form"
  ],
  "version": "1.7.2",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/j2u-ts.umd.js",
  "module": "./dist/j2u-ts.es.js",
  "exports": {
    ".": {
      "import": "./dist/j2u.es.js",
      "require": "./dist/j2u.umd.js"
    },
    "./dist/j2u.css": {
      "import": "./dist/j2u.css",
      "require": "./dist/j2u.css"
    }
  },
  "types": "./dist/main.d.ts",
  "scripts": {
    "dev": "vitepress dev docs",
    "lint": "eslint --ext .js,.ts,.vue .",
    "build": "vue-tsc && vite build",
    "prepack": "vite optimize --force"
  },
  "peerDependencies": {
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@types/node": "^20.11.28",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vuepic/vue-datepicker": "^4.0.1",
    "ajv-i18n": "^4.2.0",
    "eslint": "^8.57.0",
    "eslint-plugin-jsdoc": "^48.2.1",
    "json-schema": "^0.4.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "typescript": "^5.4.2",
    "vite": "^5.1.6",
    "vite-plugin-dts": "^3.7.3",
    "vitepress": "^1.0.0-rc.45",
    "vue": "^3.4.21",
    "vue-tsc": "^2.0.6"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "ajv-formats": "^3.0.0-rc.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/morgbn/j2u.git"
  }
}
