{
  "name": "j2u",
  "description": "Json schema Ui Utilities (Form generator, List, Filter)",
  "license": "MIT",
  "keywords": [
    "vue",
    "vuejs3",
    "json-schema",
    "ui",
    "ui-components",
    "ui-library",
    "form"
  ],
  "version": "1.7.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/j2u-ts.umd.js",
  "module": "./dist/j2u-ts.es.js",
  "exports": {
    ".": {
      "import": "./dist/j2u.es.js",
      "require": "./dist/j2u.umd.js"
    },
    "./dist/j2u.css": {
      "import": "./dist/j2u.css",
      "require": "./dist/j2u.css"
    }
  },
  "types": "./dist/main.d.ts",
  "scripts": {
    "dev": "vitepress dev docs",
    "lint": "eslint --ext .js,.ts,.vue .",
    "build": "vue-tsc && vite build",
    "prepack": "vite optimize --force"
  },
  "peerDependencies": {
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@types/node": "^18.13.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vuepic/vue-datepicker": "^4.0.1",
    "ajv-i18n": "^4.2.0",
    "eslint": "^8.56.0",
    "eslint-plugin-jsdoc": "^40.0.0",
    "json-schema": "^0.4.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vite-plugin-dts": "^2.0.0-beta.1",
    "vitepress": "^1.0.0-alpha.46",
    "vue": "^3.4.19",
    "vue-tsc": "^1.8.27"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "ajv-formats": "^3.0.0-rc.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/morgbn/j2u.git"
  }
}
